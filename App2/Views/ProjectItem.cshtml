@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.ProjectItem>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
}

<div class="row">
    <div class="col-md-1"><br /></div>
    <div class="col-md-2 bg-light" style="padding:10px; border-radius: 10px;">@Umbraco.RenderMacro("listNewestProjects")</div>
    <div class="col-md-1"><br /></div>
    <hr class="clearfix w-100 d-md-none pb-3">
    <div class="col-md-6">
        <h1>@Model.ProjectTitle</h1> <br /> <br />
        <div>@Model.ProjectDescription</div>
        <div>@Model.ProjectStatus</div>
    </div>
</div>
